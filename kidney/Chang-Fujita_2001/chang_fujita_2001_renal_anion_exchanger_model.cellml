                     <apply>
                        <plus/>
                        <apply>
                           <times/>
                           <ci>k1</ci>
                           <ci>E_e</ci>
                           <ci>HCO3_e</ci>
                        </apply>
                        <apply>
                           <minus/>
                           <apply>
                              <times/>
                              <ci>k1_</ci>
